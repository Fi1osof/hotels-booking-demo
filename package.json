    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "storybook": "^10.1.4",
    "styled-components": "^6.1.19",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@next/eslint-plugin-next": "^16.0.6",
